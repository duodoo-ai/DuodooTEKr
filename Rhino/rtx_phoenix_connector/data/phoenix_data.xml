<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="data_phoenix_token" model="rtx.phoenix.token">
            <field name="name">获取PhoenixAPI网关接口调用token</field>
            <field name="grant_type">password</field>
            <field name="username">admin</field>
            <field name="password">admin</field>
            <field name="url">http://10.0.0.1</field>
            <field name="port">14050</field>
        </record>


        <record id="data_phoenix_configurator01" model="rtx.phoenix.configurator">
            <field name="name">间隔样本数</field>
            <field name="value">10</field>
            <field name="active">True</field>
            <field name="note">由系统创建</field>
        </record>
        <record id="data_phoenix_configurator02" model="rtx.phoenix.configurator">
            <field name="name">温升系数</field>
            <field name="value">1.2</field>
            <field name="active">True</field>
            <field name="note">由系统创建</field>
        </record>

        <!-- 系统里都是按 name 字符串搜索的，所以千万不要修改name -->
        <record id='decimal_speed' model="decimal.precision">
            <field name="name">Speed</field>
            <field name="digits" eval="0"/>
        </record>
        <record id='decimal_process' model="decimal.precision">
            <field name="name">Process</field>
            <field name="digits" eval="4"/>
        </record>
        <record id='decimal_digit' model="decimal.precision">
            <field name="name">Digit</field>
            <field name="digits" eval="4"/>
        </record>
        <record id='decimal_total' model="decimal.precision">
            <field name="name">Total</field>
            <field name="digits" eval="4"/>
        </record>
    </data>
</odoo>
